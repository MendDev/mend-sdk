{
  "name": "@menddev/sdk",
  "version": "1.0.0",
  "description": "Browser + Node.js SDK for Mend",
  "main": "dist/sdk.cjs.js",
  "module": "dist/sdk.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "exports": {
    ".": {
      "require": "./dist/sdk.cjs.js",
      "import": "./dist/sdk.esm.js"
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MendDev/mend-sdk.git"
  },
  "author": "Mend",
  "keywords": [
    "mend",
    "sdk",
    "api",
    "typescript",
    "fetch"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "npm run build && npm test",
    "test": "echo \"No tests\""
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.0.0",
    "rollup": "^4.7.0",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.8.1",
    "typescript": "^5.3.3"
  }
}
